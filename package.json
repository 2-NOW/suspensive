{
  "name": "suspensive",
  "private": true,
  "license": "MIT",
  "author": "Jonghyeon Ko<manudeli.ko@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suspensive/react.git"
  },
  "bugs": {
    "url": "https://github.com/suspensive/react/issues"
  },
  "keywords": [
    "react",
    "suspense",
    "suspensive",
    "react-query"
  ],
  "workspaces": {
    "packages": [
      "configs/*",
      "docs/*",
      "examples/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build:react": "yarn workspace @suspensive/react run build",
    "build:react-query": "yarn workspace @suspensive/react-query run build",
    "publish:react": "yarn workspace @suspensive/react run publish:public",
    "publish:react-query": "yarn workspace @suspensive/react-query run publish:public",
    "build:examples:visualization": "yarn workspace @example/visualization run build",
    "start:examples:visualization": "yarn workspace @example/visualization run start",
    "dev:examples:visualization": "yarn workspace @example/visualization run dev",
    "dev:docs:react": "yarn workspace @docs/react run start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "type:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.3.0"
}
