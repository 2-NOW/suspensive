{
  "name": "@suspensive/react",
  "version": "1.5.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "rm -rf dist esm && tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "npm:publish": "yarn npm publish",
    "prepack": "yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@suspensive/babel": "workspace:^",
    "@suspensive/rollup": "workspace:^",
    "@suspensive/tsconfig": "workspace:^",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.5.1",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "import": "./esm/index.mjs",
    "main": "./dist/index.js",
    "module": "./esm/index.mjs",
    "types": "./dist/index.d.ts"
  }
}
